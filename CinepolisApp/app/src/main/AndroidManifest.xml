<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cinepolisapp" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CinepolisApp" >

        <activity
            android:name=".Vista.HomeAdminActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CinepolisApp.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Vista.BillboardAdminActivity" />
        <activity android:name=".Vista.UsersAdminActivity" />
        <activity android:name=".Vista.AlimentoAdminActivity" />
    </application>

</manifest>